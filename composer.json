{
	"name": "cpsit/formkit",
	"description": "Formkit is an extension to the TYPO3 CMS providing content elements for forms. The forms are rendered by a frontend app based on Formkit (see https://formkit.com/)",
	"homepage": "https://github.com/CPS-IT/formkit",
	"license": "GPL-3.0-or-later",
	"type": "typo3-cms-extension",
	"require": {
		"typo3/cms-core": "^11.5 || ^12.4",
		"nng/nnrestapi": "^1.4 || ^2.0"
	}
	,
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "formkit",
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	},
	"autoload": {
		"psr-4": {
			"Cpsit\\Formkit\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cpsit\\Formkit\\Tests\\": "Tests"
		}
	},
	"config": {
		"vendor-dir": ".Build/vendor/",
		"bin-dir": ".Build/bin/"
	}
}
